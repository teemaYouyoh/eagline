-
  const contactsList = [
      {
        isTargetBlank: 'false',
        image: '@/assets/img/contacts-filled-1.svg',
        href: 'tel:+48 58 351 39 14',
        title: '+48 58 351 39 14',
      },
      {
        isTargetBlank: 'false',
        image: '@/assets/img/contacts-filled-2.svg',
        href: 'mailto:info@eagline.eu',
        title: 'info@eagline.eu'
      },
      {
        isTargetBlank: 'true',
        image: '@/assets/img/contacts-filled-3.svg',
        href: 'https://maps.app.goo.gl/vpLsETSpX3dtKoEt5',
        title: 'Gdynia 81-304, Polska, ul. Śląska, 53 / B505'
      },
      {
        isTargetBlank: 'false',
        image: '@/assets/img/contacts-filled-2.svg',
        href: 'mailto:sales@eagline.eu',
        title: 'sales@eagline.eu - <span>отдел продаж</span>'
      },
  ];

article.section.contacts
  ._container
    .contacts__wrapper

      .contacts__image
        img(src=require('@/assets/img/contacts/contacts.jpg'), alt="")

      .contacts__main

        h2.contacts__title!= 'Мне нужен расчет стоимости'
        p.contacts__text.text-with-before!= 'Отправьте нам заявку с информацией о вашей задаче. В течение 24 часов мы свяжемся и предложим несколько вариантов решений. Это вас ни к чему не обязывает.'

        if contactsList.length !== 0
          ul.contacts-list
            each link, index in contactsList
              li.contacts-item.contacts-list__item
                  a.contacts-item__link(
                    href=`${link.href}`,
                    target=`${link.isTargetBlank === 'true' ? '_blank' : '_self'}`
                  )
                    span.contacts-item__label
                      img(
                        src=require(`${link.image}`),
                        alt=`contact-${index + i + 1}`
                      )

                    span.contacts-item__text!= link.title

                  if index == 0
                    span.contacts-item__socials
                      a.contacts-item__social(
                        href='#',
                        target=`${link.isTargetBlank === 'true' ? '_blank' : '_self'}`
                      )
